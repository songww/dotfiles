// vim: ft=jsonc
{
  "codeLens.enable": true,
  "coc.preferences.enableMessageDialog": true,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.silentAutoupdate": false,
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "clangd.enabled": true,
  "clangd.path": "/usr/bin/clangd",
  "clangd.checkUpdates": false,
  "clangd.inlayHints.enable": true,
  "diagnostic.autoRefresh": true,
  "diagnostic.virtualText": true,
  "diagnostic.hintSign": "ﱤ",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "hover.target": "float",
  "links.tooltip": true,
  "signature.target": "float",
  "suggest.enablePreview": true,
  "suggest.removeDuplicateItems": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "semanticTokens.filetypes": ["*"],
  "Lua.telemetry.enable": false,
  "Lua.diagnostics.globals": ["vim"],
  "rust-analyzer.enable": true,
  "rust-analyzer.lens.run": true,
  "rust-analyzer.checkOnSave.allFeatures": true,
  "rust-analyzer.completion.privateEditable.enable": true,
  "rust-analyzer.diagnostics.enableExperimental": true,
  "rust-analyzer.inlayHints.enable": true,
  "rust-analyzer.inlayHints.typeHints": true,
  "rust-analyzer.inlayHints.renderColons": true,
  "rust-analyzer.inlayHints.chainingHints": true,
  "rust-analyzer.inlayHints.reborrowHints": true,
  "rust-analyzer.inlayHints.refreshOnInsertMode": true,
  "rust-analyzer.inlayHints.closureReturnTypeHints": true,
  "rust-analyzer.inlayHints.hideNamedConstructorHints": false,
  "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "skip_trivial",
  "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
  "snippets.autoTrigger": true,
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": ["UltiSnips"]
}
